    <Link>
      <AdditionalDependencies>$(OPENCV_INSTALL)x64\vc14\lib\*.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>